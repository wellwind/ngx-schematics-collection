{
  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "form-control": {
      "alias": ["fc"],
      "description": "Create an Angular form control component.",
      "factory": "./form-control",
      "schema": "./form-control/schema.json"
    },
    "validator": {
      "description": "Create an Angular validator directive.",
